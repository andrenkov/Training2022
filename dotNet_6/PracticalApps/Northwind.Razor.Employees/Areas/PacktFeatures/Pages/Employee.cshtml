@page
@using Packt.Shared
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@model Northwind.Razor.Employees.PacktFeatures.Pages.EmployeesPageModel

<div class="row">
    <h1 class="display-2">Employees</h1>
</div>

<div class="row">
    @foreach(Employee employee in Model.Employees)
    {
        <div>
            <partial name="_Employee" model="employee" /> @*exec "partial" view "_Employee" from library Northwind.Razor.Employees with passing "employee" to it as Model*@
        </div>
    }
</div>
